{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "outDir": "./",
    "rootDir": "./"
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "node_modules",
    "*.js"
  ]
}
